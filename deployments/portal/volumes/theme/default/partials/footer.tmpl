<!-- Main footer menu used across the app -->
<footer class="acme-footer mt-auto py-3">
  <div class="container py-5">
    <div class="row align-items-center">
      <div class="col-md-4 text-center text-md-left mb-4 mb-md-0">
        <img src="/assets/images/acme_logo.png" alt="acme Logo" style="height:40px;width:auto;">
        <div class="acme-footer-copyright mt-2">Copyright Â© 2025 Acme Inc.</div>
      </div>
      <div class="col-md-8">
        <div class="row">
          {{ if GetMenus.Footer }}
            {{ range GetMenus.Footer.Children }}
              <div class="col-6 col-md-4 col-lg-3 mb-3">
                <h6 class="footer-menu mb-3">
                  <a class="footer-menu__link" href="{{.Path}}">{{.Tag}}</a>
                </h6>
                <ul class="list-unstyled">
                  {{ range .Children }}
                    <li>
                      <a href="{{.Path}}">{{.Tag}}</a>
                    </li>
                  {{ end }}
                </ul>
              </div>
            {{ end }}
          {{ end }}
        </div>
      </div>
    </div>
  </div>
</footer>
<!-- Load 3rd party Javascript assets -->
<script src="https://code.jquery.com/jquery-3.5.1.min.js" crossorigin="anonymous"></script>
<script src="https://cdn.jsdelivr.net/npm/popper.js@1.16.0/dist/umd/popper.min.js" crossorigin="anonymous"></script>
<script src="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/js/bootstrap.min.js" crossorigin="anonymous"></script>
<!-- Load Tyk Javascript assets -->
<script type="module" src="/assets/javascripts/main.js{{.previewId}}"></script>
