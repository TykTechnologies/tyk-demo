{
  "components": {},
  "info": {
    "title": "Basic Stream API",
    "version": "1.0.0"
  },
  "openapi": "3.0.3",
  "paths": {
    "/": {
      "get": {
        "operationId": "get",
        "responses": {
          "200": {
            "description": ""
          }
        }
      }
    }
  },
  "servers": [
    {
      "url": "http://tyk-gateway.localhost:8080/stream-write-stdout/"
    }
  ],
  "x-tyk-api-gateway": {
    "info": {
      "dbId": "665edf7e6fe5ee00019a40dc",
      "id": "566d32b4dd1241187b4fc25d01fbaa63",
      "name": "Stream - write to stdout",
      "orgId": "5e9d9544a1dcd60001d0ed20",
      "state": {
        "active": true
      }
    },
    "middleware": {
      "global": {
        "contextVariables": {
          "enabled": true
        },
        "trafficLogs": {
          "enabled": true
        }
      },
      "operations": {
        "get": {
          "allow": {
            "enabled": true
          }
        }
      }
    },
    "server": {
      "listenPath": {
        "strip": true,
        "value": "/stream-write-stdout/"
      }
    },
    "upstream": {
      "url": "http://httpbin/"
    }
  },
  "x-tyk-streaming": {
    "streams": {
      "test": {
        "input": {
          "generate": {
            "count": 10,
            "interval": "1s",
            "mapping": "root = \"hello world\"\n"
          },
          "label": "example_generator_input"
        },
        "output": {
          "label": "example_stdout_output",
          "stdout": {
            "codec": "lines"
          }
        }
      }
    }
  }
}
