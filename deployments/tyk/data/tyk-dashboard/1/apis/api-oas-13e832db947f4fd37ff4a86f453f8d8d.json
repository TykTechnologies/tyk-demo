{
  "components": {},
  "info": {
    "title": "Stream - http proxy with edit",
    "version": "1.0.0"
  },
  "openapi": "3.0.3",
  "paths": {},
  "servers": [
    {
      "url": "http://tyk-gateway.localhost:8080/stream-http-proxy-with-data-append/"
    }
  ],
  "x-tyk-api-gateway": {
    "info": {
      "dbId": "665edf7e6fe5ee00019a40db",
      "id": "13e832db947f4fd37ff4a86f453f8d8d",
      "name": "Stream - http proxy with data append",
      "orgId": "5e9d9544a1dcd60001d0ed20",
      "state": {
        "active": true
      }
    },
    "middleware": {
      "global": {
        "contextVariables": {
          "enabled": true
        },
        "trafficLogs": {
          "enabled": true
        }
      }
    },
    "server": {
      "listenPath": {
        "strip": true,
        "value": "/stream-http-proxy-with-data-append/"
      }
    },
    "upstream": {
      "url": "http://httpbin/"
    }
  },
  "x-tyk-streaming": {
    "streams": {
      "httpproxy": {
        "input": {
          "http_server": {
            "path": "/"
          }
        },
        "output": {
          "http_client": {
            "propagate_response": true,
            "url": "http://httpbin/post",
            "verb": "POST"
          }
        },
        "pipeline": {
          "processors": [
            {
              "mapping": "root = content().uppercase() + \" APPENDED DATA\""
            }
          ]
        }
      }
    }
  }
}
