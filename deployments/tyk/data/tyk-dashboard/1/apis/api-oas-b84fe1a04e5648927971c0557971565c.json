{
  "components": {},
  "info": {
    "title": "OAS HTTPbin",
    "version": "1.0.0"
  },
  "openapi": "3.0.3",
  "paths": {
    "/anything/validate-request-body": {
      "post": {
        "operationId": "anything/validate-request-bodypost",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "properties": {
                  "age": {
                    "description": "Age in years",
                    "minimum": 0,
                    "type": "integer"
                  },
                  "firstName": {
                    "type": "string"
                  },
                  "lastName": {
                    "type": "string"
                  }
                },
                "required": [
                  "firstName",
                  "lastName"
                ],
                "title": "Person",
                "type": "object"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": ""
          }
        },
        "summary": "Validate request body against JSON schema"
      }
    },
    "/mock-response": {
      "get": {
        "operationId": "mock-responseget",
        "responses": {
          "200": {
            "content": {
              "application/json": {
                "example": {
                  "mockBody": "hello world body"
                }
              }
            },
            "description": "Return a 200 OK",
            "headers": {
              "Mock-Header": {
                "description": "Add a mock header",
                "schema": {
                  "example": "hello world header",
                  "type": "string"
                }
              }
            }
          }
        },
        "summary": "Returns a mock response"
      }
    }
  },
  "servers": [
    {
      "url": "http://tyk-gateway.localhost:8080/oas-httpbin/"
    }
  ],
  "x-tyk-api-gateway": {
    "info": {
      "dbId": "661cfde574a7e30001847176",
      "id": "b84fe1a04e5648927971c0557971565c",
      "name": "OAS HTTPbin",
      "orgId": "5e9d9544a1dcd60001d0ed20",
      "state": {
        "active": true
      }
    },
    "middleware": {
      "global": {
        "cache": {
          "enabled": true,
          "timeout": 60
        }
      },
      "operations": {
        "anything/validate-request-bodypost": {
          "validateRequest": {
            "enabled": true,
            "errorResponseCode": 422
          }
        },
        "mock-responseget": {
          "mockResponse": {
            "enabled": true,
            "fromOASExamples": {
              "code": 200,
              "contentType": "application/json",
              "enabled": true
            }
          }
        }
      }
    },
    "server": {
      "listenPath": {
        "strip": true,
        "value": "/oas-httpbin/"
      }
    },
    "upstream": {
      "url": "http://httpbin/"
    }
  }
}
