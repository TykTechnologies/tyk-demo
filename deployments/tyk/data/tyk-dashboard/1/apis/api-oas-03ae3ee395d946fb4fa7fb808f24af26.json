{
  "components": {},
  "info": {
    "title": "Streams - HTTP to AMQP",
    "version": "1.0.0"
  },
  "openapi": "3.0.3",
  "paths": {},
  "servers": [
    {
      "url": "http://tyk-gateway.localhost:8080/streams-http-to-amqp/"
    }
  ],
  "x-tyk-api-gateway": {
    "info": {
      "dbId": "672505a3ff36fd0001e106f3",
      "id": "03ae3ee395d946fb4fa7fb808f24af26",
      "name": "Streams - HTTP to AMQP",
      "orgId": "5e9d9544a1dcd60001d0ed20",
      "state": {
        "active": true
      }
    },
    "middleware": {
      "global": {
        "contextVariables": {
          "enabled": true
        },
        "trafficLogs": {
          "enabled": true
        }
      }
    },
    "server": {
      "listenPath": {
        "strip": true,
        "value": "/streams-http-to-amqp/"
      }
    },
    "upstream": {
      "url": "http://httpbin/"
    }
  },
  "x-tyk-streaming": {
    "streams": {
      "rabbit-mq-output": {
        "input": {
          "http_server": {
            "path": "/post"
          }
        },
        "output": {
          "amqp_0_9": {
            "exchange": "my_exchange",
            "key": "my_routing_key",
            "type": "direct",
            "urls": [
              "amqp://user:password@rabbitmq:5672/"
            ]
          }
        }
      },
      "rabbitmq-input": {
        "input": {
          "amqp_0_9": {
            "consumer_tag": "tyk_streams_consumer",
            "prefetch_count": 10,
            "queue": "my_queue",
            "urls": [
              "amqp://user:password@rabbitmq:5672/"
            ]
          }
        },
        "output": {
          "http_server": {
            "stream_path": "/sse",
            "ws_path": "/ws"
          }
        }
      }
    }
  }
}
