{
    "components": {},
    "info": {
      "title": "Oauth2 Endpoint",
      "version": "1.0.0"
    },
    "openapi": "3.0.3",
    "paths": {
      "/oauth/token": {
        "post": {
          "description": "Hide the keycloak implementation path",
          "operationId": "oauth/tokenpost",
          "responses": {
            "200": {
              "description": ""
            }
          },
          "summary": "Client Token exchange"
        }
      }
    },
    "servers": [
      {
        "url": "http://tyk-gateway.localhost:8080/oauth-2.0/"
      }
    ],
    "x-tyk-api-gateway": {
      "info": {
        "dbId": "68910531f09b4200014fd0e2",
        "id": "83d51511143c4a6e7cb918ff96806afe",
        "name": "Oauth2 Endpoint",
        "orgId": "5e9d9544a1dcd60001d0ed20",
        "state": {
          "active": true
        }
      },
      "middleware": {
        "global": {
          "contextVariables": {
            "enabled": true
          },
          "cors": {
            "allowCredentials": true,
            "allowedHeaders": [
              "Accept",
              "Content-Type",
              "Origin",
              "X-Requested-With",
              "Authorization"
            ],
            "allowedMethods": [
              "GET",
              "HEAD",
              "POST"
            ],
            "allowedOrigins": [
              "*"
            ],
            "debug": true,
            "enabled": true,
            "maxAge": 24
          },
          "trafficLogs": {
            "enabled": true
          }
        },
        "operations": {
          "oauth/tokenpost": {
            "urlRewrite": {
              "enabled": true,
              "pattern": "/",
              "rewriteTo": "/realms/master/protocol/openid-connect/token"
            }
          }
        }
      },
      "server": {
        "listenPath": {
          "strip": true,
          "value": "/oauth-2.0/"
        }
      },
      "upstream": {
        "url": "http://keycloak:8180"
      }
    }
  }